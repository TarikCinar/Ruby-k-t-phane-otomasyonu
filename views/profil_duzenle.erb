<body>

<div id="colorlib-page">
  <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle"><i></i></a>
  <aside id="colorlib-aside" role="complementary" class="js-fullheight">
    <nav id="colorlib-main-menu" role="navigation">
      <ul>
        <li ><a href="/">Anasayfa</a></li>
        <% if @giris_yapilmismi %>
          <li class="colorlib-active"><a href="/profil">Profil</a></li>
        <%end %>
        <li><a href="/kitaplar">Kitaplar</a></li>
        <li><a href="/dergiler">Dergiler</a></li>
        <li><a href="/e-kitaplar">E-kitaplar</a></li>
        <li><a href="/iletisim">İletişim</a></li>
        <% if not @giris_yapilmismi %>
          <li><a href="/login">Giriş yap</a></li>
          <li><a href="/kayit">Kayıt ol</a></li>
        <%else %>
          <li><a href="/logout">Çıkış yap</a></li>
        <% end %>

      </ul>
    </nav>

    <div class="colorlib-footer">
      <h1 id="colorlib-logo" class="mb-4"><a href="/" style="background-image: url(images/bg_1.jpg); font-size: 50px;margin-bottom: 50px">Ruby <span>Library</span></a></h1>
    </div>
  </aside> <!-- END COLORLIB-ASIDE -->
  <div id="colorlib-main">
      <div class="container-fluid" >
        <div class="row justify-content-md-start" style="margin-left: 100px;margin-top: 50px">
          <div class="col-lg-7">

            <form action="/edit" method="POST" enctype="multipart/form-data">
              <div class="form-group">
                <label for="largeInput" style="font-weight: bold">İsim</label>
                <input type="text" class="form-control form-control" id="defaultInput" placeholder="isim" name="isim" value="<%=@user[0][2] %>">
              </div>

              <div class="form-group">
                <label for="largeInput" style="font-weight: bold">Soyisim</label>
                <input type="text" class="form-control form-control" id="defaultInput" placeholder="soyisim" name="soyisim" value="<%=@user[0][3] %>">
              </div>

              <div class="form-group">
                <label for="largeInput" style="font-weight: bold">Email</label>
                <input type="email" class="form-control form-control" id="defaultInput" placeholder="email" name="email" value="<%=@user[0][4] %>">
              </div>

              <div class="form-group">
                <label for="largeInput" style="font-weight: bold">Şifre</label>
                <input type="password" class="form-control form-control" id="defaultInput" placeholder="basım tarihi" name="sifre" value="<%=@user[0][5] %>">
              </div><br>

              <div class="form-group">
                <label for="largeInput" style="font-weight: bold">Resim</label><br>
                <input type="file" name="file">
              </div><br>

              <div class="form-group" style="text-align: start">
                <button type="submit" class="btn btn-primary" style="width: 150px">Kaydet</button>
              </div>
            </form>
          </div>
        </div>
      </div>
  </div><!-- END COLORLIB-MAIN -->
</div><!-- END COLORLIB-PAGE -->

